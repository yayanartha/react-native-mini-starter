{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb-base"
    ],
    "rules": {
        // overrides
        "class-methods-use-this": 0,
        "no-use-before-define": 0,
        "no-mixed-operators": 0,
        "no-tabs": 0,
        "indent": 0,
        "arrow-body-style": 0,
        "arrow-parens": 0,
        "global-require": 0,
        "no-underscore-dangle": 0,
        "no-multi-spaces": 0,
        "no-console": 0,
        "object-property-newline": 0,
        "react/no-unescaped-entities": 0,
        "camelcase": 0,
        "brace-style": 0,
        "padded-blocks": 0,
        "no-nested-ternary": 0
    }
}
